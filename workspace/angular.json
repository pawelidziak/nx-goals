            ],
            "exclude": ["**/node_modules/**", "!libs/goal-list/**"]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "libs/goal-list/jest.config.js",
            "tsConfig": "libs/goal-list/tsconfig.spec.json",
            "setupFile": "libs/goal-list/src/test-setup.ts"
          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "api": {

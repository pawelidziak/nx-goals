          }
        }
      },
      "schematics": {
        "@nrwl/angular:component": {
          "styleext": "scss"
        }
      }
    },
    "ngrx-forms": {
      "projectType": "library",
      "root": "libs/ngrx-forms",
      "sourceRoot": "libs/ngrx-forms/src",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {

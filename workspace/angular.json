        }
      }
    },
    "toolbar": {
      "projectType": "library",
      "root": "libs/toolbar",
      "sourceRoot": "libs/toolbar/src",
      "prefix": "workspace",
      "architect": {
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
